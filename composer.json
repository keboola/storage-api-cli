{
  "name": "keboola/storage-api-cli",
  "description": "Keboola Storage API command line interface",
  "authors": [
    {
      "name": "Keboola",
      "email": "devel@keboola.com"
    }
  ],
  "license": "MIT",
  "require": {
    "keboola/kbc-project-backup": "^0.0.2",
    "keboola/kbc-project-restore": "^1.1",
    "keboola/php-temp": "^1.0",
    "keboola/storage-api-client": "^10.1.1",
    "keboola/symfony-console-nested-formatter-helper": "^0.1",
    "symfony/console": "^2.8",
    "symfony/filesystem": "^2.8"
  },
  "require-dev": {
    "phpstan/phpstan-shim": "^0.9.2",
    "phpunit/phpunit": "^5.7",
    "slevomat/coding-standard": "^4.4",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "tests": "phpunit --debug",
    "phpcbf": "phpcbf -n --ignore=vendor --extensions=php .",
    "phpcs": "phpcs -n --ignore=vendor --extensions=php .",
    "phpstan": "phpstan analyse ./src ./tests --level=max --no-progress -c phpstan.neon",
    "build": [
      "@phpcs",
      "@phpstan",
      "@tests"
    ],
    "ci": [
      "@composer validate --no-check-all --strict",
      "@build"
    ]
  },
  "autoload": {
    "psr-4": {
      "Keboola\\StorageApi\\Cli\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Keboola\\StorageApi\\Cli\\Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
